{
    "name": "haitaopai",
    "version": "1.1.4",
    "description": "Hai Tao Pai",
    "repository": "https://github.com/KevinWangzzz/haitaopai",
    "main": "lib/index.js",
    "scripts": {
        "clean": "rimraf ./node_modules",
        "lint": "eslint src/**/*.js --fix && gulp lint-style",
        "build:css": "gulp less2css",
        "build:js": "rollup --config",
        "build:sdk": "gulp generate-bff-sdk",
        "build": "npm run lint && rimraf ./dist && npm run build:css && npm run build:js && npm run build:sdk && npm run copy-public",
        "watch:less": "gulp watch-less",
        "watch:js": "rollup -c -w",
        "copy-public": "rimraf ./public && gulp copy-common-files",
        "dev": "npm run build && node ./src/server/dev.js",
        "dev.js": "node ./src/server/dev.js",
        "start": "npm run build && node ./src/server/server.js",
        "deploy:haitaopai": "node --max_old_space_size=8192M ./src/server/server.js",
        "build-ui": "npm run build && npm run copy-public"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@ant-design/pro-components": "^1.1.6",
        "@babel/eslint-parser": "^7.12.13",
        "@babel/plugin-proposal-class-properties": "^7.16.7",
        "@babel/plugin-transform-runtime": "^7.16.7",
        "@babel/preset-env": "^7.16.7",
        "@babel/preset-react": "^7.16.7",
        "@babel/register": "^7.15.3",
        "@hapi/boom": "^9.1.2",
        "@hapi/cookie": "^11.0.2",
        "@hapi/h2o2": "^9.1.0",
        "@hapi/hapi": "^20.2.2",
        "@hapi/inert": "^6.0.5",
        "antd": "^4.16.13",
        "axios": "^0.21.1",
        "blipp": "4.0.2",
        "crypto-js": "^4.1.1",
        "lodash": "^4.17.20",
        "mobx": "^6.3.3",
        "mobx-react-lite": "^3.2.1",
        "prop-types": "^15.7.2",
        "react": "17.0.2",
        "react-dom": "17.0.2"
    },
    "devDependencies": {
        "@rollup/plugin-babel": "^5.3.0",
        "@rollup/plugin-commonjs": "^17.1.0",
        "@rollup/plugin-node-resolve": "^11.2.0",
        "@rollup/plugin-replace": "^2.3.4",
        "eslint-config-airbnb": "18.2.1",
        "eslint-plugin-promise": "^6.0.0",
        "gulp": "^4.0.2",
        "gulp-less": "^4.0.1",
        "gulp-rename": "^2.0.0",
        "gulp-replace": "^1.0.0",
        "gulp-stylelint": "^13.0.0",
        "less-plugin-autoprefix": "^2.0.0",
        "less-plugin-clean-css": "^1.5.1",
        "rollup": "^2.39.0",
        "rollup-plugin-json": "^4.0.0",
        "rollup-plugin-node-builtins": "^2.1.2",
        "rollup-plugin-node-globals": "^1.4.0",
        "rollup-plugin-terser": "^7.0.2"
    },
    "files": [
        "dist",
        "lib"
    ]
}
